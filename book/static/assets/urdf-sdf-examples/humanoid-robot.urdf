    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="0.78" effort="100" velocity="1"/>
  </joint>

  <joint name="right_knee_joint" type="revolute">
    <parent link="right_upper_leg"/>
    <child link="right_lower_leg"/>
    <origin xyz="0 0 -0.4" rpy="0 0 0"/>
